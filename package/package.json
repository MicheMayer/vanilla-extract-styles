{
  "name": "@michemayer/vanilla-extract-styles",
  "version": "1.0.14",
  "description": "Basic styling for web components utilizing vanilla-extract",
  "author": {
    "name": "Miche Mayer",
    "url": "https://miche-mayer.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michemayer/vanilla-extract-styles"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [],
